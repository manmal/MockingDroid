<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.manuelmaly.mockingdroid" android:versionCode="1"
	android:versionName="1.0">
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<application android:icon="@drawable/icon" android:label="@string/app_name" android:theme="@style/MockingDroidTheme">
		<activity android:name=".HTML" android:label="@string/app_name">
		</activity>

		<activity android:name="Main">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
	<activity android:name="com.lamerman.FileDialog"></activity>
	<provider android:name="com.manuelmaly.mockingdroid.LocalFileContentProvider"
          android:authorities="com.manuelmaly.localfile"
      />
</application>


	<uses-sdk android:minSdkVersion="4" android:targetSdkVersion="4"></uses-sdk>

</manifest> 